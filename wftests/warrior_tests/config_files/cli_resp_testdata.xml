<data>
        <global>
                <command_params
                start=".*" end="\$" timeout="5" >
                </command_params>
                <verifications>
                </verifications>
            <keys>
                <gk resp_pattern_req="Hi this"/>
            </keys>

        </global>
        <!--
        Use for testing cli cmd response behavior
    resp_req="y", always on unless turn-off by setting to n
    resp_ref=<personal flag> default is commands number in testdata block,
             i.e. if it is the third command then it will be 3 and so on,
    resp_pat_req=<response pattern to look for> no defaults,
                  if not given will just return the whole response
                  Will not take affects if resp_req=n
        -->
        <!--
        command by title:
        -->
        <!-- response turn-off and resp_pat_req not in use-->


    <testdata execute="yes" row="" title="title_1">
        <command resp_pat_req="Desktop" resp_ref="td1_cmd1" resp_req="" resp_pat_key="value" send="ls -F" />
        <command resp_pat_req="Templates" resp_ref="td1_cmd2" resp_req="" resp_pat_key="temp" send="ls -F"  />
        <command resp_pat_req="Music" resp_ref="td1_cmd3" resp_req="" send="ls -F" />
    </testdata>

    <testdata execute="yes"  title="title_4">
        <command send="echo 'Hi this is title_4 and executing one multiple reference'"
                     resp_keys="gk, k1, k2"
                     resp_pat_key = "test"
                     resp_ref="lsoutK1,lsoutK2,lsoutK3"
                     inorder_resp_ref="no"  session="session1" sys="sys1" />
            <k1 resp_pattern_req="title_4"/>
            <k2 resp_pattern_req="one multiple reference"/>
    </testdata>



</data>
